<%- include("partials/header"); -%>

<body class="about-body">
  <section class="container-round" >
    <div class="shape-rounded" style="background-image: url( <%= data['background-image'] %> );" >
      <header>
        <div class="container px-4 px-lg-5 d-flex h-100 align-items-center justify-content-center">
          <div class="d-flex justify-content-center">
            <div class="text-center">
              <h1 class="mx-auto my-0 text-uppercase logo-heading"><img class="main-logo" src="img/brandmark-design.png" alt=""></h1>
              <h2 class="h2 mx-auto mt-2 mb-5">Your Local Web Design and SEO Professional</h2>
            </div>
          </div>
        </div>
      </header>
    </div>
   
  </section>
  <section class="info-section">
        <h2 class="">What is it?</h2>
        <p class="about-text">
          <%= data["What is it?"] %>
        </p>
        <h1>Why you need it</h1>
          <p>
            <%= data["Why you need it."] %>
            </p>
  </section> 
</body>

<!-- Footer-->
<footer class="about-footer footer small text-center"><div class="container px-4 px-lg-5">Copyright &copy; Colorad Designs 2022</div></footer>
